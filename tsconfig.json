{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "outDir": "ts-output",
    "jsx": "react",
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules"
  ],
  "filesGlob": [
    "./src/**/*.tsx",
